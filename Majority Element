static const bool Init=[](){
    ios_base::sync_with_stdio(false);
    cout.tie(0);
    cin.tie(0);
    return true;
}();
class Solution {
public:
    int majorityElement(vector<int>& nums) {
      int count=0;
      int candidate=nums[0];

      for(int i=0;i<nums.size();i++){
        if(count==0){
            candidate=nums[i];
        }
        if(nums[i]==candidate){
            count++;
        }
        else{
            count--;
        }
      }
        return candidate;
    }
};
